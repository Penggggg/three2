

.p-trip-enter2 {
    
}

.te2-content {
    height: 100vh;
    display: flex;
    flex-direction: column;
}

.te2-content-container {
    flex: 1;
    padding: 5rpx 0 10rpx;
    overflow-y: scroll;
    box-sizing: border-box;
}

.te2-recommand-block {
    height: 100%;
    overflow-y: scroll;
}

.te2-classify-container {
    padding: 0rpx 10rpx 15rpx 10rpx;
}

.te2-loading-tips {
    color: #999;
    font-size: 26rpx;
    text-align: center;
    padding-top: 200rpx;
}

.te2-search-con {
    padding: 0 20rpx;
    margin-bottom: 10rpx;
}

.te2-search-block {
    display: flex;
    padding: 5rpx 20rpx 5rpx; 
    align-items: center;
    border-radius: 30px;
    background: #f5f5f5;
    box-sizing: border-box;
}

.te2-icon {
    margin-right: 15rpx;
}

.te2-search-text {
    color: #bfbfbf;
    font-size: 22rpx;
    letter-spacing: 0.05em;
}

.te2-classify-contaienr {
    margin: 0rpx 0 5rpx;
    overflow-x: scroll;
    position: relative;
    border-bottom: 3px solid #fafafa;
}

.te2-classify-block {
    padding: 20rpx 25rpx;
    white-space: nowrap;
}

.te2-classify-item {
    color: #4d4d4d;
    font-size: 24rpx;
    line-height: 32rpx;
    margin-right: 40rpx;
    display: inline-block;
    letter-spacing: 0.05em;
}

.te2-classify-item.active {
    color: #333;
    font-weight: bold;
}

.te2-recommand-logo-swiper {
    height: 350rpx;
}

.te2-recommond-logo-item {
    height: 350rpx;
    position: relative;
    box-sizing: border-box;
}

.te2-recommond-logo-item.line {
    border: 4px solid #000;
}

.te2-recommond-logo {
    width: 100%;
    height: 100%;
}

.te2-logo-block {
    top: 50%;
    right: 80rpx;
    display: flex;
    position: absolute;
    align-items: center;
    flex-direction: column;
    transform: translate( 0, -50% );
}

.te2-logo-avatar {
    width: 200rpx;
    height: 200rpx;
    border-radius: 50%;
    border: 2px solid #fff;
    box-shadow: 0 0 15px rgba( 0, 0, 0, 0.35 );
}

.te2-logo-avatar.small {
    width: 40rpx;
    height: 40rpx;
    border: none;
    margin-left: 20rpx;
}

.te2-tips-btn-block {
    display: flex;
    margin-top: 20rpx;
    max-height: 30rpx;
    align-items: center;
    border-radius: 5px;
    padding: 10rpx 20rpx;
    background: rgba( 255, 255, 255, 0.8);
    box-shadow: 3px 3px 25px rgba( 0, 0, 0, 0.2 );    
}

.te2-tips-btn {
    font-size: 22rpx;
    color: #4d4d4d;
    letter-spacing: 0.02em;
}

.te2-header-icon {
    width: 30rpx;
}

.te2-trip-info {
    display: flex;
    overflow: visible;
    align-items: flex-end;
    box-sizing: border-box;
    margin-top: 5rpx;
}

.trip-decorate {
    color: #333;
    font-size: 30rpx;
    font-weight: bold;
    position: relative;
    letter-spacing: 0.1em;
    margin-right: 20rpx;
}

.trip-decorate:after {
    left: -10%;
    width: 110%;
    height: 20rpx;
    content: '';
    bottom: 5rpx;
    position: absolute;
    border-radius: 30rpx;
    background: rgba( 255,107,149, 0.22 );
}

.trip-title {
    color: #333;
    font-size: 34rpx;
    font-weight: bold;
    letter-spacing: 0.02em;
    margin-right: 20rpx;
}

.trip-date {
    color: #333;
    font-size: 22rpx;
    letter-spacing: 0.02em;
}

.te2-pin-block {
    top: 50%;
    left: 50rpx;
    position: absolute;
    transform: translate( 0, -50% );
}

.te2-pin-title {
    color: #333;
    font-size: 34rpx;
    letter-spacing: 0.05em;
    width: 350rpx;
    overflow: hidden; 
    display: -webkit-box; 
    -webkit-line-clamp: 1;
    word-break: break-all;
    text-overflow: ellipsis;
    -webkit-box-orient: vertical;
}

.te2-pin-title-big {
    color: #333;
    font-size: 46rpx;
    font-weight: bold;
    letter-spacing: 0.05em;
    width: 350rpx;
    overflow: hidden; 
    display: -webkit-box; 
    -webkit-line-clamp: 1;
    word-break: break-all;
    text-overflow: ellipsis;
    -webkit-box-orient: vertical;
}

.te2-pin-price-block {
    display: flex;
    margin-top: 30rpx;
    align-items: flex-end;
}

.te2-pin-price-big {
    color: #ee7283; 
    font-size: 50rpx;
    font-weight: bold;
    margin-right: 25rpx;
    /* text-shadow: 0 0 3px rgba( 0, 0, 0, 0.2 ); */
}

.te2-pin-price {
    color: #ee7283;
    font-size: 36rpx;
    font-weight: bold;
    line-height: 60rpx;
    position: relative;
    margin-right: 15rpx;
    letter-spacing: 0.1em;
    /* text-decoration: line-through; */
}

.te2-pin-price.line:after {
    top: 55%;
    left: 50%;
    height: 5rpx;
    width: 120%;
    content: '';
    position: absolute;
    background: #ee7283;
    transform: translate( -50%, -50% );
}

.te2-zhushou-block {
    color: #fff;
    font-size: 22rpx;
    margin-top: 20rpx;
    position: relative;
    padding: 5rpx 15rpx;
    border-radius: 30px;
    display: inline-block;
    letter-spacing: 0.08em;
    transition: all ease 0.4s;
    background: rgba( 0, 0, 0, 0.3 );
}

.te2-zhushou-block.hide {
    opacity: 0;
}


.te2-zhushou-block.fixed {
    top: -30rpx;
    left: -100rpx;
    position: absolute;
}

.te2-zhushou-block.fixed:after {
    bottom: -11rpx;
    right: 25rpx;
    content: '';
    width: 0;
    height: 0;
    position: absolute;
    border-left: 10rpx solid transparent;
    border-right: 10rpx solid transparent;
    border-top: 12rpx solid rgba( 0, 0, 0, 0.25 );
}

.te2-zhushou-tips1 {
    color: #fff;
    font-size: 22rpx;
}

.te2-zhushou-tips2 {
    color: #fff;
    margin-top: 5rpx;
    font-size: 24rpx;
}

.te2-recommond-good-con {
    top: 50%;
    right: 85rpx;
    position: absolute;
    border-radius: 2px;
    border: 3px solid #fff;
    box-shadow: 0px 0px 15px rgba( 0, 0, 0, 0.15 );
    transform: translate( 0, -50% );
}

.te2-pinest-img-con {
    /* width:190rpx; */
    width: 0rpx;
    height:225rpx;
    overflow: hidden;
}

.te2-pinest-img {
    width: 100%;
    border-radius: 2px;
    height: 100%;
}

.te2-pinest-text-block {
    display: flex;
    align-items: center;
    justify-content: center;
    box-sizing: border-box;
    padding: 2rpx 0;
    background: rgba( 255, 255, 255, 0.85 );
}

.te2-pinest-text {
    color: #333;
    font-size: 24rpx;
    font-weight: bold;
    letter-spacing: 0.1em;
}

.te2-pinest-icon {
    width: 30rpx;
    max-height: 30rpx;
    margin-left: 10rpx;
}

.test {
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
}

.te2-entry-block {
    display: flex;
    box-sizing: border-box;
    padding: 45rpx 30rpx 15rpx 30rpx;
}

.te2-entry-item {
    flex: 1;
    display: flex;
    align-items: center;
    flex-direction: column;
}

.te2-entry-icon {
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
    /* box-shadow: 3px 3px 15px rgba( 0, 0, 0, 0.15 ); */
}

.te2-entry-label {
    color: #333;
    font-size: 25rpx;
    margin-top: 15rpx;
    font-weight: bold;
    letter-spacing: 0.08em;
}

.te2-new-block {
    display: flex;
    flex-wrap: wrap;
    padding: 0 20rpx;
    box-sizing: border-box;
}

.te2-new-item {
    display: flex;
    align-content: center;
    flex-direction: column;
    margin: 10rpx;
    border-radius: 3px;
    border: 1px solid rgba( 0, 0, 0, 0.05 );
}

.te2-new-item-img {
    width: 210rpx;
    height: 250rpx;
}

.te2-new-item-info {
    width: 210rpx;
    /* height: 120rpx; */
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    padding: 10rpx 15rpx 10rpx;
    box-sizing: border-box;
}

.te2-new-item-title {
    font-size: 22rpx;
    line-height: 36rpx;
    letter-spacing: 0.05em;
    color: #666;
    overflow: hidden; 
    display: -webkit-box; 
    -webkit-line-clamp: 1;
    word-break: break-all;
    text-overflow: ellipsis;
    -webkit-box-orient: vertical;
}

.te2-new-price-block {
    display: flex;
    align-items: flex-end;
}

.te2-new-price {
    color: #aa3f37;
    font-size: 24rpx;
    margin: 10rpx 10rpx 0 0;
}

.te2-new-price2 {
    font-size: 24rpx;
    color: rgba( 0, 0, 0, 0.35 );
}

.te2-good-list {
    padding: 0rpx 25rpx 30rpx 25rpx;
}

.te2-top-banner-block {
    padding: 0 38rpx;
    margin: 15rpx 0 0rpx;
    position: relative;
}

.te2-top-banner-img {
    width: 100%;
    height: 200rpx;
    border-radius: 10rpx;
}

.te2-top-banner-info {
    top: 45%;
    left: 100rpx;
    position: absolute;
    transform: translate( 0, -50% );
}

.te2-top-banner-title {
    color: #333;
    font-size: 31rpx;
    font-weight: bold;
    letter-spacing: 0.08em;
}

.te2-top-banner-tips {
    color: #333;
    font-size: 26rpx;
    letter-spacing: 0.08em;
}