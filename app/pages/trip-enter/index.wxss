}

.te-container {
    display: flex;
    width: 100vw;
    height: 100vh;
    box-sizing: border-box;
    flex-direction: column;
}

.te-content {
    flex: 1;
    /* padding-top: 70rpx; */
    overflow-y: scroll;
    box-sizing: border-box;
}

.te-btn-container {
    padding: 0 20rpx 25rpx;
}

.te-btn {
    color: #ff0077;
    padding: 15rpx 0;
    font-size: 36rpx;
    text-align: center;
    border-radius: 6px;
    background: #ffe8ee;
}

.te-recommond-list {
    width: 100vw;
    margin-top: 5rpx;
    /* padding: 5rpx 40rpx; */
    white-space: nowrap;
    overflow-x: scroll;
    margin: 0 0 20rpx;
    box-sizing: border-box;
}

.te-recommond-list::-webkit-scrollbar {
    display: none;
}

.te-recommond-list::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

.te-recommond-item {
    display: inline-block;
}

.te-recommond-item:not(:last-child) {
    margin-right: 70rpx;
}

.te-recommond-item2 {
    width: 50vw;
    display: inline-block;
}

.te-logo-bar {
    display: flex;
    margin-top: 15rpx;
    margin-bottom: 10rpx;
    padding: 0 20rpx;
    box-sizing: border-box;
    justify-content: center;
    position: relative;
}

.te-logo-bar-text-block {
    top: 50%;
    left: 85rpx;
    position: absolute;
    transform: translate( 0, -50% );
}

.te-logo-bar-title {
    display: flex;
    color: #fff;
    font-size: 42rpx;
    line-height: 42rpx;
    letter-spacing: 0.01em;
    align-items: flex-end;
    text-shadow: 0px 0px 5px rgba( 0, 0, 0, 0.25 );
}

.te-logo-bar-tips {
    display: inline;
    color: #fff;
    font-size: 21rpx;
    line-height: 26rpx;
    margin-left: 10rpx;
    letter-spacing: 0.01em;
    text-shadow: 0px 0px 5px rgba( 0, 0, 0, 0.25 );
}

.te-logo-bar-btn {
    color: #fff;
    font-size: 22rpx;
    font-weight: bold;
    height: 60rpx;
    width: 60rpx;
    border-radius: 50%;
    top: 50%;
    right: 45rpx;
    position: absolute;
    transform: translate( 0, -50% );
    background-image: linear-gradient(to right, #ff758c 0%, #ff7eb3 100%);
    background-image: linear-gradient(to right, #f77062 0%, #fe5196 100%);
    background-image: linear-gradient(-45deg, #FFC796 0%, #FF6B95 100%);
    box-shadow: 3px 3px 25px rgba(0, 0, 0, 0.15);
}

.te-logo-bar-btn.re {
    right: 0;
    position: relative !important;
    transform: translate( 0, 0 );
}

.te-logo-bar-icon {
    top: 50%;
    left: 50%;
    position: absolute;
    transform: translate( 0%, -50% );
    animation: mymove 1.2s linear infinite;
    -webkit-animation: mymove 1.2s linear infinite; 
}

.te-logo-bar-icon.notanimate {
    animation: none;
    -webkit-animation: none;
    transform: translate( -50%, -50% );
}

@keyframes mymove
{
    from {left:5rpx;opacity: 1;}
    to {left:35rpx;opacity: 0.2;}
}

@-webkit-keyframes mymove 
{
    from {left:5rpx;opacity: 1;}
    to {left:35rpx;opacity: 0.2;}
}

.te-bar-img {
    width: 100%;
    border-radius: 6px;
