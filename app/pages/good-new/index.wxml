<view class="p-good-new">
    <scroll-view
        scroll-y
        class="pgn-scroll"
        lower-threshold="100"
        bindscrolltolower="fetchRank"
    >
        <!-- logo -->
        <my-banner-1
            img="{{ ipAvatar }}"
            title1="{{ bannerTips$[ 0 ]}}"
            title2="{{ bannerTips$[ 1 ]}}"
        />
        <!-- 列表 -->
        <view class="pgn-good-list">
            <my-good-item-9
                wx:key="{{ index }}"
                wx:for="{{ rankList }}"
                good="{{ item }}"
            />
        </view>

    </scroll-view>
</view>