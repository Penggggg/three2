<view class="page-order-list">

    <!-- 上方tab -->
    <view class="pol-tabs">
        <view
            wx:for="{{ tabs }}"
            bindtap="onTab"
            data-index="{{ index }}"
            wx:key="{{ index }}"
            class="pol-tabs-item {{ active === index ? 'active' : '' }}"
        >
            {{ item.label }}
        </view>
    </view>

    <!-- 下方列表 -->
    <view class="pol-content-block">
        
    </view>

</view>