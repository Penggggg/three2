.con-trip-order-shoppinglist {
    min-height: 100vh;
    background: #fafafa;
}

.tos-money-block {
    display: flex;
    align-items: center;
    background: #ff5777;
    flex-direction: column;
    justify-content: center;
    padding: 10rpx 0 100rpx;
}

.tos-money-title {
    font-size: 20rpx;
    color: rgba( 255, 255, 255, 0.6 );
}

.tos-money-i {
    color: #fff;
    font-size: 78rpx;
    font-weight: bold;
    margin: 25rpx 0;
}

.tos-order-count {
    color: #fff;
    font-size: 22rpx;
}